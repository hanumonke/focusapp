{
  "expo": {
    "name": "fokus",
    "slug": "fokus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fokus-logo.png",
    "scheme": "fokus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hanumonke.fokus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fokus-logo.png",
        "backgroundColor": "#a9cce3"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hanumonke.fokus"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "defaultChannel": "default",
          "color": "#5893ba",
          "sounds": [
            
            "./assets/sounds/x8bit.wav",
            "./assets/sounds/x8bit02.wav",
            "./assets/sounds/alarm.wav",
            "./assets/sounds/campanas.wav",
            "./assets/sounds/clocktower.wav",
            "./assets/sounds/guerra.wav",
            "./assets/sounds/guitarra.wav",
            "./assets/sounds/hellooooyou.wav",
            "./assets/sounds/nuclearalarm.wav",
            "./assets/sounds/pipipipi.wav",
            "./assets/sounds/pupetfnaf.wav",
            "./assets/sounds/suspenso.wav",
            "./assets/sounds/tuntun.wav",
            "./assets/sounds/vibracion.wav",
            "./assets/sounds/xilofono.wav"
          ]
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fokus-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#a9cce3"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "useLegacyPackaging": true
          }
        }
      ],
      "expo-document-picker",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8a849272-92a6-4fc5-9e09-59165015ac91"
      }
    }
  }
}