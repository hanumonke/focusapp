{
  "expo": {
    "name": "fokus",
    "slug": "fokus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fokus-logo.png",
    "scheme": "fokus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hanumonke.fokus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fokus-logo.png",
        "backgroundColor": "#a9cce3"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hanumonke.fokus"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": [
            
            "./assets/sounds/x8bit.mp3",
            "./assets/sounds/x8bit02.mp3",
            "./assets/sounds/alarm.mp3",
            "./assets/sounds/campanas.mp3",
            "./assets/sounds/clocktower.mp3",
            "./assets/sounds/guerra.mp3",
            "./assets/sounds/guitarra.mp3",
            "./assets/sounds/hellooooyou.mp3",
            "./assets/sounds/nuclearalarm.mp3",
            "./assets/sounds/pipipipi.mp3",
            "./assets/sounds/pupetfnaf.mp3",
            "./assets/sounds/suspenso.mp3",
            "./assets/sounds/tuntun.mp3",
            "./assets/sounds/vibracion.mp3",
            "./assets/sounds/xilofono.mp3"
          ]
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fokus-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#a9cce3"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "useLegacyPackaging": true
          }
        }
      ],
      "expo-document-picker",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8a849272-92a6-4fc5-9e09-59165015ac91"
      }
    }
  }
}